<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout android:layout_width="fill_parent"
	android:layout_height="fill_parent" 
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/theme1" android:background="@drawable/homebgblue">
	<RelativeLayout android:layout_alignParentTop="true"
		android:id="@+id/header"
		android:background="@drawable/headerbar" 
		android:layout_width="fill_parent" 
		android:layout_height="40dip">
		<ImageView android:id="@+id/headerhomeimage" 
		android:background="@drawable/headerhomeicon" 
		android:layout_height="22dip" 
		android:layout_width="22dip" 
		android:layout_marginLeft="12dip" 
		android:layout_marginTop="10dip">
		</ImageView>
		<TextView android:layout_width="wrap_content" 
		android:text="E-Book Home" 
		android:id="@+id/headertext" 
		android:layout_height="30dip" 
		android:layout_marginLeft="38dip" 
		android:layout_marginTop="10dip" android:textStyle="bold"
		android:textColor="#ffffff">
		</TextView>
		<ImageView android:background="@drawable/activewindowicon"
			android:clickable="true" 
			android:id="@+id/activewindow"
			android:layout_height="18dip" 
			android:layout_width="68dip" 
			android:layout_marginLeft="210dip" 
			android:layout_marginTop="24dip">
		</ImageView>
		

<ImageView android:layout_height="22dip" 
	android:layout_width="24dip" 
	android:layout_marginLeft="420dip" 
	android:layout_marginTop="10dip" 
	android:background="@drawable/headerwifiicon" 
	android:id="@+id/headerwifiImage">
</ImageView>
<ImageView android:layout_width="36dip" 
	android:layout_height="22dip" 
	android:layout_marginLeft="460dip" 
	android:layout_marginTop="10dip" 
	android:background="@drawable/headerbatteryicon" 
	android:id="@+id/batteryIconImage">
</ImageView>
		
	
<TextView android:layout_width="wrap_content" 
android:layout_height="30dip" 
android:text="06:30" 
android:layout_marginLeft="335dip" 
android:id="@+id/Time" 
android:layout_marginTop="10dip" android:textStyle="bold" android:textColor="#ffffff">
</TextView>

</RelativeLayout>

	<RelativeLayout android:layout_below="@id/header"
		android:id="@+id/library" 
		android:layout_width="fill_parent" android:layout_height="530dip" 
		>
		
		<ImageView android:id="@+id/ImageViewLibrary" 
		android:background="@drawable/mylibrarylinkimage"
		android:layout_marginTop="22dip"
		android:layout_marginLeft="18dip"
		android:layout_width="38dip" 
		android:layout_height="38dip">
		</ImageView>
		<TextView android:layout_width="wrap_content" 
		android:id="@+id/librarytext" 
		android:text="My Library" 
		android:layout_marginLeft="60dip" 
		android:layout_height="wrap_content" 
		android:layout_marginTop="35dip" android:textStyle="bold" android:textSize="17dip"
		android:textColor="#ffffff">
		</TextView>
		<Button android:id="@+id/gotolibrary" 
		android:background="@drawable/mylibrarylinkbutton" 
		android:layout_height="25dip" 
		android:layout_width="25dip" 
		android:layout_marginTop="26dip" android:layout_marginLeft="460dip"></Button>
	<ImageView android:layout_below="@id/ImageViewLibrary" 
	android:id="@+id/horizentallineaftergotolib" 
	android:layout_height="4dip" 
	android:layout_width="fill_parent" 
	android:layout_marginLeft="9dip" 
	android:background="@drawable/dividerline">
	</ImageView>
	<GridView xmlns:android="http://schemas.android.com/apk/res/android" 
	android:id="@+id/libraryGrid" 
	android:numColumns="3" 
	android:stretchMode="columnWidth" 
	android:layout_below="@+id/gotolibrary" android:columnWidth="288dp" 
	android:layout_width="wrap_content" android:layout_height="260dip" 
	android:layout_marginTop="20dip"
	android:layout_marginLeft="20dip">
	</GridView>
	<AbsoluteLayout android:id="@+id/newspaperLayout" 
	android:layout_height="230dip" 
	android:layout_width="fill_parent" 
	android:layout_below="@+id/libraryGrid">
	<ImageView android:layout_width="150dip" 
	android:layout_height="132dip" 
	android:layout_x="50dip"
	android:id="@+id/newspaper1" 
	android:background="@drawable/newspapericon">
	</ImageView>
	
	<ImageView android:layout_width="50dip" 
	android:layout_height="132dip" 
	android:layout_x="270dip" 
	android:id="@+id/magazine1"
	>
	</ImageView>
	<ImageView android:layout_width="50dip" 
	android:layout_height="132dip" 
	android:layout_x="340dip" 
	android:id="@+id/magazine2"
	>
	</ImageView>
	<ImageView android:layout_width="50dip" 
	android:layout_height="132dip" 
	android:layout_x="300dip" 
	android:id="@+id/magazine3"
	>
	</ImageView>
	
	
	</AbsoluteLayout>



</RelativeLayout>

	<RelativeLayout android:layout_below="@id/library"
		android:id="@+id/horizental" 
		android:layout_height="1dip" 
		android:layout_width="fill_parent">
		</RelativeLayout>
		
	<RelativeLayout android:layout_below="@id/horizental"
		android:id="@+id/browser" 
		android:layout_height="278dip" 
		android:layout_width="270dip" android:layout_marginLeft="10dip">
		<ImageView android:id="@+id/ImageViewBrowser" 
		android:background="@drawable/browserlinkimage"
		android:layout_marginLeft="11dip"
		android:layout_width="38dip" 
		android:layout_height="38dip">
		</ImageView>
		<TextView android:layout_width="wrap_content" 
		android:id="@+id/browsertext" 
		android:text="Browser"
		android:textColor="#ffffff"
		android:layout_marginLeft="53dip" 
		android:layout_height="wrap_content" 
		android:layout_marginTop="10dip" android:textStyle="bold" android:textSize="17dip">
		</TextView>
		<ImageView android:id="@+id/gotomybrowser" 
		android:background="@drawable/browserlinkbutton" 
		android:layout_height="25dip" 
		android:layout_width="25dip" 
		android:layout_marginLeft="200dip"
		android:layout_marginTop="7dip">
		</ImageView>
		<ImageView android:layout_below="@id/ImageViewBrowser" 
	android:id="@+id/horizentallineaftergotolib" 
	android:layout_height="4dip" 
	android:layout_width="462dip" 
	android:layout_marginLeft="9dip" 
	android:background="@drawable/dividerline">
	</ImageView>
	<AbsoluteLayout android:id="@+id/browsertheme3" 
	android:layout_height="fill_parent" 
	android:layout_width="fill_parent" 
	android:layout_below="@+id/horizentallineaftergotolib">
	<WebView android:layout_width="125dip" 
	android:layout_height="100dip"  
	android:layout_y="26dip" 
	android:layout_x="30dip" 
	android:id="@+id/WebView1Theme1" 
	android:scrollbars="none"
	android:visibility="invisible">
	</WebView>
	<WebView android:layout_width="125dip" 
	android:layout_height="100dip" 
	android:layout_x="60dip" 
	android:layout_y="80dip" 
	android:id="@+id/WebView2Theme1" 
	android:scrollbars="none"
	android:visibility="invisible"
	>
	</WebView>
	</AbsoluteLayout>

</RelativeLayout>
	<RelativeLayout android:layout_below="@+id/horizental"
		android:layout_alignParentRight="true" 
		android:id="@+id/organizer" 
		android:layout_marginLeft="21dip" 
		android:layout_marginRight="10dip" 
		android:layout_width="219dip" 
		android:layout_alignRight="@+id/browser" 
		android:layout_height="278dip">
		<ImageView android:id="@+id/ImageViewOrganizer" 
		android:background="@drawable/organizerlinkimage"
		android:layout_width="38dip" 
		android:layout_height="38dip">
		</ImageView>
		<TextView android:layout_width="wrap_content" 
		android:layout_marginTop="10dip" 
		android:id="@+id/organizertext" 
		android:text="Organizer" 
		android:textColor="#ffffff"
		android:layout_height="wrap_content" 
		android:layout_marginLeft="44dip" 
		android:textStyle="bold" 
		android:textSize="17dip">
		</TextView>
		<ImageView android:id="@+id/gotomyorganizer" 
		android:background="@drawable/organizerlinkbutton" 
		android:layout_height="25dip" 
		android:layout_width="25dip" 
		android:layout_marginLeft="180dip"
		android:layout_marginTop="7dip"></ImageView>
			<ImageView android:layout_below="@id/ImageViewOrganizer" 
	android:id="@+id/horizentallineaftergotolib" 
	android:layout_height="4dip" 
	android:layout_width="462dip" 
	android:layout_marginLeft="9dip" 
	android:background="@drawable/dividerline">
	</ImageView>
	<AbsoluteLayout android:id="@+id/organizertheme3" 
	android:layout_height="fill_parent" 
	android:layout_width="fill_parent" 
	android:layout_below="@+id/horizentallineaftergotolib">
	<ImageView android:layout_width="wrap_content" 
	android:layout_height="wrap_content" 
	android:layout_y="50dip" 
	
	android:layout_x="30dip" 
	android:id="@+id/note1theme3">
	</ImageView>
	<ImageView android:layout_width="wrap_content" 
	android:layout_height="wrap_content" 
	android:layout_x="70dip" android:layout_y="80dip" 
	android:id="@+id/note2theme3">
	</ImageView>
	</AbsoluteLayout>
	

</RelativeLayout>
	
	<RelativeLayout android:id="@+id/footer" 
		android:background="@drawable/footericonsbg" 
		android:layout_height="46dip" 
		android:layout_width="300dip" 
		android:layout_marginLeft="100dip" android:layout_below="@+id/browser">

		<Button android:id="@+id/settings"
			android:layout_height="33dip" 
			android:layout_width="33dip" 
			android:background="@drawable/footersettingsicon" 
			android:layout_marginLeft="46dip" 
			android:layout_marginTop="10dip">
			</Button>  
	<ImageView android:layout_height="33dip" 
	android:layout_width="33dip" 
	android:id="@+id/searchatfooter" 
	android:background="@drawable/footersearchicon" 
	android:layout_marginLeft="102dip" 
	android:layout_marginTop="10dip">
	</ImageView>
<ImageView android:layout_height="33dip" 
android:layout_width="33dip" 
android:id="@+id/bookmarkfooter" 
android:background="@drawable/footerbookmarkicon" 
android:layout_marginLeft="154dip" 
android:layout_marginTop="10dip">
</ImageView>
<ImageView android:layout_height="33dip" 
android:layout_width="33dip" 
android:id="@+id/tagfooter" 
android:background="@drawable/footertagicon" 
android:layout_marginLeft="211dip" 
android:layout_marginTop="10dip">
</ImageView>
</RelativeLayout>
</RelativeLayout>
