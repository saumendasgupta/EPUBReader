<?xml version="1.0" encoding="utf-8"?>



<RelativeLayout android:layout_width="fill_parent"
	android:layout_height="fill_parent" 
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/theme2" android:background="@drawable/homebg">
	<RelativeLayout android:layout_alignParentTop="true"
		android:id="@+id/header"
		android:background="@drawable/headerbar" 
		android:layout_width="480dip" 
		android:layout_height="40dip">
		<ImageView android:id="@+id/headerhomeimage" 
		android:background="@drawable/headerhomeicon" 
		android:layout_height="21dip" 
		android:layout_width="25dip" 
		android:layout_marginLeft="13dip" 
		android:layout_marginTop="10dip">
		</ImageView>
		<TextView android:layout_width="wrap_content" 
		android:text="E-Book Home" 
		android:id="@+id/headertext" 
		android:layout_height="30dip" 
		android:layout_marginLeft="40dip" 
		android:layout_marginTop="15dip">
		</TextView>
		<ImageView android:background="@drawable/activewindowicon"
			android:clickable="true" 
			android:id="@+id/activewindow"
			android:layout_height="18dip" android:layout_width="60dip" android:layout_marginLeft="210dip" android:layout_marginTop="20dip">
		</ImageView>
		

<ImageView android:layout_height="20dip" 
	android:layout_width="28dip" 
	android:layout_marginLeft="372dip" 
	android:layout_marginTop="12dip" 
	android:background="@drawable/headerwifiicon" 
	android:id="@+id/headerwifiImage">
</ImageView>
<ImageView android:layout_width="35dip" 
	android:layout_height="17dip" 
	android:layout_marginLeft="429dip" 
	android:layout_marginTop="12dip" 
	android:background="@drawable/headerbatteryicon" 
	android:id="@+id/batteryIconImage">
</ImageView>
<TextView android:layout_width="wrap_content" 
android:layout_height="30dip" 
android:text="06:30" 
android:layout_marginLeft="325dip" 
android:id="@+id/Time" 
android:layout_marginTop="15dip">
</TextView>		
	
</RelativeLayout>

	<RelativeLayout android:layout_below="@id/header"
		android:id="@+id/organizer" 
		android:layout_width="fill_parent" 
		android:layout_height="460dip">
		<TextView android:layout_width="wrap_content" 
		android:layout_marginTop="35dip" 
		android:id="@+id/organizertext" 
		android:text="Organizer" 
		android:layout_height="wrap_content" 
		android:layout_marginLeft="30dip"
		android:textSize="20dip" android:textStyle="bold">
		</TextView>
		<ImageView android:id="@+id/gotomyorganizer" 
		android:background="@drawable/organizerlinkbutton" 
		android:layout_height="32dip" 
		android:layout_width="32dip" 
		android:layout_marginLeft="420dip" 
		android:layout_marginTop="26dip"></ImageView>
	<ImageView android:layout_below="@id/gotomyorganizer" 
	android:id="@+id/horizentallineaftergotolib" 
	android:layout_height="4dip" 
	android:layout_width="462dip" 
	android:layout_marginLeft="9dip" 
	android:background="@drawable/dividerline">
	</ImageView>
	<GridView xmlns:android="http://schemas.android.com/apk/res/android" 
	android:id="@+id/OrganizerGrid" 
	android:layout_width="fill_parent" 
	android:verticalSpacing="10dp" 
	android:horizontalSpacing="10dp" 
	android:numColumns="3" 
	android:columnWidth="60dp" 
	android:stretchMode="columnWidth" 
	android:gravity="center" 
	android:layout_below="@+id/gotomyorganizer" android:layout_height="fill_parent"></GridView>
	


</RelativeLayout>

	<RelativeLayout android:layout_below="@id/organizer"
		android:id="@+id/horizental" 
		android:layout_height="1dip" 
		android:layout_width="fill_parent">
		</RelativeLayout>
		<RelativeLayout android:layout_below="@id/horizental" 
		android:id="@+id/library" android:layout_height="258dip" 
		android:layout_width="250dip" android:layout_marginLeft="10dip">
		<Button android:id="@+id/gotolibrary" 
		android:background="@drawable/mylibrarylinkbutton" 
		android:layout_height="32dip" 
		android:layout_width="32dip" 
		android:layout_marginLeft="167dip">
		</Button>
		<TextView android:layout_width="wrap_content" 
		android:id="@+id/librarytext" 
		android:text="My Library" 
		android:layout_marginLeft="30dip" 
		android:layout_height="wrap_content" 
		android:layout_marginTop="10dip" android:textStyle="bold" android:textSize="20dip">
		</TextView>
		<ImageView android:layout_below="@id/gotolibrary" 
		android:id="@+id/horizentallineaftergotolib" 
		android:layout_height="4dip" android:layout_width="462dip" 
		android:layout_marginLeft="9dip" 
		android:background="@drawable/dividerline">
	</ImageView>
	<AbsoluteLayout android:id="@+id/library_theme2" 
	android:layout_height="fill_parent" 
	android:layout_width="fill_parent" 
	android:layout_below="@+id/horizentallineaftergotolib">
	<ImageView android:layout_width="wrap_content" 
	android:layout_height="wrap_content" 
	android:layout_y="50dip" 
	android:layout_x="60dip" 
	android:id="@+id/book1_theme2">
	</ImageView>
	<ImageView android:layout_width="wrap_content" 
	android:layout_height="wrap_content" 
	android:layout_x="90dip" android:layout_y="80dip" 
	android:id="@+id/book2_theme2">
	</ImageView>
	</AbsoluteLayout>
	<GridView xmlns:android="http://schemas.android.com/apk/res/android" 
	android:id="@+id/libraryGrid" android:layout_width="fill_parent" 
	android:layout_height="fill_parent" android:padding="10dp" 
	android:verticalSpacing="10dp" android:horizontalSpacing="10dp" 
	android:numColumns="3" android:columnWidth="60dp" android:stretchMode="columnWidth" 
	android:gravity="center" android:layout_below="@+id/horizentallineaftergotolib" 
	android:visibility="invisible">


</GridView>
</RelativeLayout>
	
	<RelativeLayout android:layout_below="@+id/horizental"
		android:layout_alignParentRight="true" 
		android:id="@+id/browser" 
		android:layout_marginLeft="21dip" 
		android:layout_marginRight="10dip" 
		android:layout_width="219dip" 
		android:layout_alignRight="@+id/library" 
		android:layout_height="258dip">
		<TextView android:layout_width="wrap_content" 
		android:id="@+id/browsertext" 
		android:text="Browser" 
		android:layout_marginLeft="30dip" 
		android:layout_height="wrap_content" 
		android:layout_marginTop="10dip" android:textStyle="bold" android:textSize="20dip">
		</TextView>
		<ImageView android:id="@+id/gotomybrowser" 
		android:background="@drawable/browserlinkbutton" 
		android:layout_height="32dip" 
		android:layout_width="32dip" 
		android:layout_marginLeft="170dip"></ImageView>
			<ImageView android:layout_below="@id/gotomybrowser" 
	android:id="@+id/horizentallineaftergotolib" 
	android:layout_height="4dip" 
	android:layout_width="462dip" 
	android:layout_marginLeft="9dip" 
	android:background="@drawable/dividerline">
	</ImageView>
	<AbsoluteLayout android:id="@+id/browsertheme3" 
	android:layout_height="fill_parent" 
	android:layout_width="fill_parent" 
	android:layout_below="@+id/horizentallineaftergotolib">
	<ImageView android:layout_width="wrap_content" 
	android:layout_height="wrap_content" 
	android:layout_y="50dip" 
	
	android:layout_x="60dip" 
	android:id="@+id/note1theme3" android:background="@drawable/browsericon00">
	</ImageView>
	<ImageView android:layout_width="wrap_content" 
	android:layout_height="wrap_content" 
	android:layout_x="90dip" android:layout_y="80dip" 
	android:id="@+id/note2theme3">
	</ImageView>
	</AbsoluteLayout>
</RelativeLayout>
	
	<RelativeLayout android:id="@+id/footer" 
		android:background="@drawable/footericonsbg" 
		android:layout_height="41dip" 
		android:layout_width="223dip" 
		android:layout_marginLeft="128dip" android:layout_below="@+id/browser">

		<Button android:id="@+id/settings"
			android:layout_height="23dip" 
			android:layout_width="23dip" 
			android:background="@drawable/footersettingsicon" 
			android:layout_marginLeft="35dip" 
			android:layout_marginTop="10dip">
			</Button>
	<ImageView android:layout_height="23dip" 
	android:layout_width="28dip" 
	android:id="@+id/searchatfooter" 
	android:background="@drawable/footersearchicon" 
	android:layout_marginLeft="89dip" 
	android:layout_marginTop="10dip">
	</ImageView>
<ImageView android:layout_height="23dip" 
android:layout_width="20dip" 
android:id="@+id/bookmarkfooter" 
android:background="@drawable/footerbookmarkicon" 
android:layout_marginLeft="128dip" 
android:layout_marginTop="10dip">
</ImageView>
<ImageView android:layout_height="28dip" 
android:layout_width="27dip" 
android:id="@+id/tagfooter" 
android:background="@drawable/footertagicon" 
android:layout_marginLeft="169dip" 
android:layout_marginTop="10dip">
</ImageView>
</RelativeLayout>
</RelativeLayout>